{
  "name": "@d4kmor/tree-model",
  "version": "0.1.0",
  "description": "Manipulate and traverse tree-like structures in javascript.",
  "keywords": [
    "tree",
    "hierarchy",
    "browser",
    "node"
  ],
  "homepage": "http://jnuno.com/tree-model-js",
  "repository": "https://github.com/joaonuno/tree-model-js",
  "bugs": "https://github.com/joaonuno/tree-model-js/issues",
  "license": "MIT",
  "author": "Jo√£o Nuno Silva <jnss81@gmail.com> (http://jnuno.com)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./dist-types/types/index.d.ts"
    }
  },
  "scripts": {
    "test": "mocha",
    "lint": "eslint src/*.js test/*.js",
    "lint:prettier": "prettier --check *.js test/*.js",
    "preversion": "npm test && npm run lint",
    "postversion": "echo 'Dont forget to publish to npm...'",
    "dist": "rm -rf dist && mkdir -p dist && browserify index.js -o dist/TreeModel.js -s TreeModel && uglifyjs dist/TreeModel.js > dist/TreeModel-min.js",
    "format": "prettier --write *.js test/*.js",
    "types": "run-s types:clear types:copy types:build",
    "types:build": "tsc --build",
    "types:clear": "rimraf dist-types/",
    "types:copy": "copyfiles \"./types/**/*.d.ts\" dist-types/"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/sinon": "^10.0.5",
    "chai": "^4",
    "copyfiles": "^2.4.1",
    "eslint": "^8",
    "eslint-config-prettier": "^8.3.0",
    "mocha": "^9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sinon": "^5",
    "typescript": "^4.6.0-dev.20211107"
  },
  "types": "dist-types/types/index.d.ts",
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  }
}
